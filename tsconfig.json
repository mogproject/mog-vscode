{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "outDir": "out",
        "rootDir": ".",
        "noImplicitAny": true,
        "suppressImplicitAnyIndexErrors": true,
        "lib": [
            "es6"
        ],
        "sourceMap": true,
        "strictNullChecks": false,
        "experimentalDecorators": true
    },
    "exclude": [
        "node_modules",
        ".vscode-test"
    ]
}
